<h1>Users</h1>
<table class="table">
  <thead>
    <tr>
      <th>image</th>
      <th>name</th>
    </tr>
  </thead>
  <% @users.each do |user| %>
  <tbody>
    <tr>
      <td>
        <%= attachment_image_tag @user, :profile_image, :fill, 20, 20, format: 'jpeg', fallback: "no_image.jpg" %>
      </td>
      <td><%= user.name %></td>
      <td><%= link_to 'Show', user_path(current_user.id) %></td>
    </tr>
  </tbody>
  <% end %>
</table>
